{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "flowtype/require-valid-file-annotation": [0],
    "react/jsx-uses-vars": [2]
  },
  "globals": {
    "React": false,
    "ReactDOM": false
  },
  "env": {
    "browser": true
  }
}
